{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IceEngine": {
    "EndPointAddress": "https://localhost:43000",
    "Methods": {
      "SubmitQuery": "/api/IceEngine/SubmitQuery",
      "FeedBack": "/api/IceEngine/FeedBack",
      "QueryStatus": "/api/IceEngine/Status"
    }
  },
  "FMSClient1": {
    "EndPointAddress": "https://localhost:43500",
    "Methods": {
      "SubmitQuery": "/rest/api/SubmitQuery",
      "FeedBack": "/rest/api/Feedback",
      "QueryStatus": "/rest/api/Status"
    }
  },

  "FMSClient": {
    "EndPointAddress": "https://localhost:43500",
    "WebMethods": [
      {
        "Name": "submitQuery",
        "Route": "/rest/api/SubmitQuery"
      },
      {
        "Name": "FeedBack",
        "Route": "/rest/api/Feedback"
      },
      {
        "Name":  "QueryStatus","Route": "/rest/api/status"
      }
    ]
  },
  "IceClientReceiver": {
    "EndPointAddress": "https://localhost:44310",
    "WebMethods": [
      {
        "Name": "SubmitQuery",
        "Route": "/api/receiver/submitQuery"
      },
      {
        "Name": "FmsClentSubmitQuery",
        "Route": "/api/receiver/ForwardQueryToFMSClient"
      }
    ]
  }


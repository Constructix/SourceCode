{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "IceEngine": {
    "EndPointAddress": "https://localhost:43000",
    "WebMethods": [
      {
        "Name": "SubmitQuery",
        "Route": "/api/IceEngine/SubmitQuery"
      },
      {
        "Name": "FeedBack",
        "Route": "/api/IceEngine/FeedBack"
      },
      {
        "Name": "QueryStatus", 
        "Route": "/api/IceEngine/Status"
      }
    ]
  },
  "FMSClient": {
    "EndPointAddress": "https://localhost:43500",
    "WebMethods": [
      {
        "Name": "submitQuery",
        "Route": "/rest/api/SubmitQuery"
      },
      {
        "Name": "FeedBack",
        "Route": "/rest/api/Feedback"
      },
      {
        "Name":  "QueryStatus","Route": "/rest/api/status"
      }
    ]
  },
  "IceClientReceiver": {
    "EndPointAddress": "https://localhost:44310",
    "WebMethods": [
      {
        "Name": "SubmitQuery",
        "Route": "/api/receiver/submitQuery"
      },
      {
        "Name": "FmsClentSubmitQuery",
        "Route": "/api/receiver/ForwardQueryToFMSClient"
      }
    ]
  }

}
